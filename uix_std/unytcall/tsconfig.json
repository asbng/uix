{
    "exclude": [],
    "compilerOptions": {
        "module": "esnext",
        "target": "ES2020",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "downlevelIteration": true,
        "useDefineForClassFields": true,
        "lib": [
            "esnext",
            "dom"
        ]
    }
}